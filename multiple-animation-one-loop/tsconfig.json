{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es2020",
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "exclude": [
    "node_modules"
  ]
}
